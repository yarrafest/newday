<div class="mt-10 grid gap-10 md:grid-cols-1 lg:grid-cols-2">
  <Card color="indigo" size="sm">
    <Fragment slot="image">
      <Picture
        src={GenEntImgOne}
        alt="Feature image"
        formats={["avif", "webp"]}
        class="h-full w-full object-contain"
        loading="lazy"
      />
    </Fragment>
    <span slot="badge">Anderson St</span>
    <span slot="title">DJ Keep Diggin’</span>
    <span slot="desc">
      Playing funk, disco, house, pop at Hopheads on Anderson St from 11AM - 4PM
    </span>
  </Card>

  <Card color="indigo" size="sm">
    <Fragment slot="image">
      <Picture
        src={GenEntImgTwo}
        alt="Feature image"
        formats={["avif", "webp"]}
